{
  "name": "custodiancustweb",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "webpack -p --progress --colors --config webpack.config.prod.js",
    "build_dev": "webpack -p --progress --colors --config webpack.config.dev.js",
    "build_debug": "webpack -p --watch --progress --colors --config webpack.config.debug.js",
    "prod": "NODE_ENV=production nohup node ./bin/CustodianCustWeb &>/dev/null &",
    "dev": "NODE_ENV=development nohup node ./bin/CustodianCustWeb &>/dev/null &",
    "debug": "NODE_ENV=development DEBUG=CustodianCustWeb:* nodemon ./bin/CustodianCustWeb",
    "win_prod": "set NODE_ENV=production&& node ./bin/CustodianCustWeb",
    "win_dev": "set NODE_ENV=development&& node ./bin/CustodianCustWeb",
    "win_debug": "set NODE_ENV=development&& set DEBUG=CustodianCustWeb:*&&  nodemon ./bin/CustodianCustWeb",
    "win_c_debug": "concurrently --kill-others \"npm run build_debug\" \"npm run win_debug\"",
    "forever_prod": "NODE_ENV=production forever start --id CustodianCustWeb -o ./log/out.log -e ./log/err.log -c node ./bin/CustodianCustWeb",
    "forever_dev": "NODE_ENV=development forever start --id CustodianCustWeb -o ./log/out.log -e ./log/err.log -c node ./bin/CustodianCustWeb",
    "win_forever_dev": "set NODE_ENV=development&& set DEBUG=CustodianCustWeb:*&&  forever start --id CustodianCustWeb -o ./log/out.log -e ./log/err.log -c  node ./bin/CustodianCustWeb",
    "ci_function_prod": "NODE_ENV=production DEBUG= node ./ci_tools/CICommand",
    "ci_function_dev": "NODE_ENV=development DEBUG= node ./ci_tools/CICommand",
    "eslint_debug": "eslint **/*.js ./bin/CustodianCustWeb --ignore-pattern node_modules/ --ignore-pattern docs/",
    "eslint_fix": "eslint **/*.js ./bin/CustodianCustWeb --ignore-pattern node_modules/ --ignore-pattern docs/ --ignore-pattern public/ --fix"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-runtime": "^6.26.0",
    "body-parser": "~1.17.1",
    "bootstrap": "^4.1.1",
    "bootstrap-select": "^1.13.1",
    "connect-mongo": "^2.0.1",
    "connect-redis": "^3.3.3",
    "cookie-parser": "~1.4.3",
    "date-format": "^1.2.0",
    "debug": "~2.6.3",
    "ejs": "~2.5.6",
    "express": "~4.15.2",
    "express-session": "^1.15.6",
    "forever": "^0.15.3",
    "helmet": "^3.13.0",
    "jquery": "^3.3.1",
    "morgan": "~1.8.1",
    "octicons": "^8.0.0",
    "popper.js": "^1.14.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-day-picker": "^7.1.10",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "serve-favicon": "~2.4.2",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^3.6.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.9.1",
    "nodemon": "^1.17.5",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.12.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.8",
    "webpack-merge": "^4.1.3"
  }
}
